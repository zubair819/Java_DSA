package com.dsa.in;
import java.util.*;
public class Finding_duplicate_no {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the size of an array");
		int n=sc.nextInt();
		int a[]=new int[n+1];
		for(int i=1;i<=n;i++) {
			System.out.println("Eneter the value for element"+i);
			a[i]=sc.nextInt();
		}

		int count=0;
		int val=0;
		for(int i=1;i<n;i++) {
			for (int j=i+1;j<=n;j++) {
				if (a[j]==a[i]) {
					count+=1;
					val=a[j];
				}
			}
			if (count!=0) break;
		}
		System.out.println("the Duplicate value is"+val);
	}
}
