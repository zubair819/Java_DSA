package com.dsa.in;
import java.util.*;
public class Finding_duplicate_no {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the size of an array");
		int n=sc.nextInt();
		int a[]=new int[n+1];
		for(int i=1;i<=n;i++) {
			System.out.println("Eneter the value for element"+i);
			a[i]=sc.nextInt();
		}

		//sorting array using selection sort 
		for(int i=1;i<n;i++) {
			int min_index=i;
			int min_value=a[i];
			for (int j=i+1;j<=n;j++) {
				if (a[j]<a[min_index]) {
					min_index=j;
					min_value=a[j];
				}
			}
			a[min_index]=a[i];
			a[i]=min_value;
		}
		
	}
}
